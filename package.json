{
  "name": "ngxs-effects",
  "version": "0.0.3",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladborsh/ngxs-effects.git"
  },
  "main": "./dist/ngxs-effect/bundles/ngxs-effect.umd.js",
  "types": "./dist/ngxs-effect/ngxs-effect.d.ts",
  "files": [
    "dist/ngxs-effect"
  ],
  "keywords": [
    "redux",
    "state",
    "ngxs",
    "angular",
    "ng",
    "effects",
    "ngxs-effects",
    "ngxs-plugin",
    "ngxs-ngrxs-effects"
  ],
  "scripts": {
    "ng": "ng",
    "start": "ng serve demo-app",
    "lint": "npm run ngxs-effect:lint",
    "test": "npm run ngxs-effect:test:watch",
    "build": "npm run ngxs-effect:build",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "demo-app:build": "ng build demo-app",
    "demo-app:test": "ng test demo-app",
    "demo-app:lint": "npx eslint -c .eslintrc.js 'projects/demo-app/**/*.ts' --fix",
    "ngxs-effect:build": "ng build ngxs-effect",
    "ngxs-effect:test": "ng test ngxs-effect --configuration=ci",
    "ngxs-effect:lint": "npx eslint -c .eslintrc.js 'projects/ngxs-effect/**/*.ts' --fix",
    "ngxs-effect:test:watch": "ng test ngxs-effect",
    "release": "standard-version",
    "release:patch": "npm run release -- --release-as patch",
    "release:minor": "npm run release -- --release-as minor",
    "release:major": "npm run release -- --release-as major",
    "publish": "npm publish"
  },
  "dependencies": {
    "@angular/animations": "~8.2.0",
    "@angular/common": "~8.2.0",
    "@angular/compiler": "~8.2.0",
    "@angular/core": "~8.2.0",
    "@angular/forms": "~8.2.0",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "@ngxs/logger-plugin": "~3.6.2-dev.master-14ab2bf",
    "@ngxs/store": "~3.6.2-dev.master-14ab2bf",
    "reflect-metadata": "~0.1.13",
    "rxjs": "~6.4.0",
    "tslib": "~1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.802.0",
    "@angular-devkit/build-ng-packagr": "~0.802.2",
    "@angular/cli": "~8.2.0",
    "@angular/compiler-cli": "~8.2.0",
    "@angular/language-service": "~8.2.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "codelyzer": "~5.0.0",
    "commitizen": "^4.0.4",
    "conventional-changelog-cli": "^2.0.31",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.0",
    "karma-mocha-reporter": "^2.2.5",
    "ng-packagr": "^5.3.0",
    "protractor": "~5.4.0",
    "standard-version": "^7.1.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.36.0",
    "tslint": "^5.18.0",
    "typescript": "~3.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run demo-app:lint && npm run ngxs-effect:lint"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
